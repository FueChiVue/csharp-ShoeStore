@Html.Partial("header.cshtml")
<body>
  <div class="container">
    <div class="jumbotron">
      <h1>Shoe Store</h1>
    </div>
    <div class="container">
      <h2>Are you sure you want to delete this store?</h2>
      <h4>@Model.GetName()</h4>
      <form action="/store/delete/@Model.GetId()" method="post">
        <input type="hidden" name="_method" value="DELETE">
        <button class="btn btn-danger" type="submit">Delete</button>
      </form>
      <h4><a href="/">Back to main page</a></h4>
    </div>
  </div>
</body>
